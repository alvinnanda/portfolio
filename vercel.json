{
    "routes": [
    {
      "src": "^/assets/(.*)\\.js$",
      "headers": { "content-type": "application/javascript" }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}